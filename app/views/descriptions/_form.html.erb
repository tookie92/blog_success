<%= simple_form_for(@description) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :profession %>
    <%= f.input :adresse %>
    <%= f.input :situation %>
    <%= f.input :qjs %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
